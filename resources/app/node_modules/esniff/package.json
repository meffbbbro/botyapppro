{
  "name": "esniff",
  "version": "2.0.1",
  "description": "Low footprint ECMAScript source code parser",
  "author": "Mariusz Nowak <medyk@medikoo.com> (http://www.medikoo.com/)",
  "repository": "medikoo/esniff",
  "dependencies": {
    "d": "^1.0.1",
    "es5-ext": "^0.10.62",
    "event-emitter": "^0.3.5",
    "type": "^2.7.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-medikoo": "^4.2.0",
    "esprima": "^4.0.1",
    "github-release-from-cc-changelog": "^2.3.0",
    "nyc": "^15.1.0",
    "prettier-elastic": "^2.7.1",
    "tad": "^3.1.1"
  },
  "eslintIgnore": [
    "/coverage",
    "/test/__playground"
  ],
  "prettier": {
    "printWidth": 100,
    "tabWidth": 4,
    "overrides": [
      {
        "files": [
          "*.md",
          "*.yml"
        ],
        "options": {
          "tabWidth": 2
        }
      }
    ]
  },
  "engines": {
    "node": ">=0.10"
  },
  "license": "ISC"
}